<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="EditInfo_Page.EditInfoController">
    <Label layoutX="105.0" layoutY="31.0" text="Name" textFill="#1d0101">
        <font>
            <Font name="System Bold" size="18.0"/>
        </font>
    </Label>
    <Label layoutX="97.0" layoutY="263.0" prefHeight="27.0" prefWidth="81.0" text="Position" textFill="#120505">
        <font>
            <Font name="System Bold" size="18.0"/>
        </font>
    </Label>
    <Label layoutX="66.0" layoutY="207.0" text="Phone Number" textFill="#240505">
        <font>
            <Font name="System Bold" size="18.0"/>
        </font>
    </Label>
    <Label layoutX="90.0" layoutY="144.0" text="Password" textFill="#190404">
        <font>
            <Font name="System Bold" size="18.0"/>
        </font>
    </Label>
    <Label layoutX="107.0" layoutY="84.0" text="Email" textFill="#1b0202">
        <font>
            <Font name="System Bold" size="18.0"/>
        </font>
    </Label>
    <TextField fx:id="editName" layoutX="233.0" layoutY="32.0" prefHeight="25.0" prefWidth="219.0"
               style="-fx-background-color: #D3D3D3;"/>
    <TextField fx:id="editPos" layoutX="233.0" layoutY="264.0" prefHeight="25.0" prefWidth="219.0"
               style="-fx-background-color: #D3D3D3;"/>
    <TextField fx:id="editNum" layoutX="233.0" layoutY="208.0" prefHeight="25.0" prefWidth="219.0"
               style="-fx-background-color: #D3D3D3;"/>
    <TextField fx:id="editEmail" layoutX="233.0" layoutY="85.0" prefHeight="25.0" prefWidth="219.0"
               style="-fx-background-color: #D3D3D3;"/>

    <PasswordField fx:id="editPass" layoutX="233.0" layoutY="147.0" prefHeight="25.0" prefWidth="219.0"
                   style="-fx-background-color: #D3D3D3;"/>
    <Label layoutX="88.0" layoutY="322.0" text="Username" textFill="#120505">
        <font>
          <Font name="System Bold" size="18.0"/>
        </font>
    </Label>
    <TextField fx:id="editUname" layoutX="233.0" layoutY="323.0" prefHeight="25.0" prefWidth="219.0"
               style="-fx-background-color: #D3D3D3;"/>
    <Button layoutX="471.0" layoutY="185.0" mnemonicParsing="false" onAction="#handleUpdate"
            style="-fx-background-color: D9DEE8;" text="Update Info">
        <font>
          <Font name="System Bold" size="14.0"/>
        </font>
    </Button>
    <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToFront" text="Back"/>
</AnchorPane>
